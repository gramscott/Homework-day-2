# # EXERCISE A

stops = [ "Croy", "Cumbernauld", "Falkirk High", "Linlithgow", "Livingston", "Haymarket" ]

stops.append("Edinburgh Waverley")
stops[0] = "Glasgow Queen St"
stops.insert(3, "Polmont")
print(stops[4])
stops.remove("Livingston")
stops.pop(1)
print(len(stops))
stops.sort()
stops.sort(reverse = True)

for stop in stops:
    print (stop)
